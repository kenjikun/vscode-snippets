  "parentheses": {
    "prefix": "(",
    "body": ["\\left($1\\right)"]
  },
  "brackets": {
    "prefix": "[",
    "body": ["\\left[$1\\right]"]
  },
  "braces": {
    "prefix": "{",
    "body": ["\\left\\{$1\\right\\\\}"]
  },
  "angle brackets": {
    "prefix": "<",
    "body": ["\\left<$1\\right>"]
  },
  "pmatrix": {
    "prefix": "pmat",
    "body": [
      "\\begin{pmatrix}",
      "\t{$1} & {$2} \\\\\\",
      "\t{$3} & {$4}",
      "\\end{pmatrix}"
    ]
  },
  "pvec": {
    "prefix": "pvec",
    "body": ["\\begin{pmatrix}", "\t{$1} \\\\\\", "\t{$2}", "\\end{pmatrix}"]
  },
  "eqnarray": {
    "prefix": "arr",
    "body": ["\\begin{array}{rl}", "\t${1:H \\psi = E \\psi}", "\\end{array}"]
  },
  "bm": {
    "prefix": "b",
    "body": ["\\bm{$1}"]
  },
